{
  "name": "champagneroom",
  "private": true,
  "version": "1.0.0",
  "description": "Champagne Room",
  "repository": "",
  "license": "UNLICENSED",
  "scripts": {
    "start": "yarn run dev",
    "dev": "cross-env NODE_ENV=development vite",
    "build": "cross-env NODE_ENV=production vite build && npx workbox generateSW workbox-config.cjs",
    "build-capacitor-ios": "cross-env NODE_ENV=production vite build && npx cap copy ios",
    "build-capacitor-android": "cross-env NODE_ENV=production vite build && npx cap copy android",
    "postinstall": "cpy --flat ./node_modules/framework7-icons/fonts/*.* ./src/fonts/ && cpy --flat ./node_modules/material-icons/iconfont/*.* ./src/fonts/",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "ionic:build": "npm run build",
    "ionic:serve": "npm run start"
  },
  "browserslist": [
    "IOS >= 13",
    "Safari >= 13",
    "last 5 Chrome versions",
    "last 5 Firefox versions",
    "Samsung >= 12"
  ],
  "type": "module",
  "dependencies": {
    "@capacitor/android": "4.6.0",
    "@capacitor/app": "^4.1.1",
    "@capacitor/browser": "^4.1.0",
    "@capacitor/camera": "^4.1.4",
    "@capacitor/clipboard": "^4.1.0",
    "@capacitor/core": "4.6.0",
    "@capacitor/ios": "4.6.0",
    "@capacitor/keyboard": "^4.1.0",
    "@capacitor/preferences": "^4.0.2",
    "@capacitor/share": "^4.1.0",
    "@capacitor/splash-screen": "^4.1.2",
    "@capacitor/status-bar": "4.1.1",
    "@ionic/pwa-elements": "^3.1.1",
    "@xstate/svelte": "^2.0.1",
    "dom7": "^4.0.4",
    "framework7": "^7.0.9",
    "framework7-icons": "^5.0.5",
    "framework7-svelte": "^7.0.9",
    "i18n-possessive": "^1.0.0",
    "keycode-js": "^3.1.0",
    "konsta": "^1.0.2",
    "material-icons": "^1.12.2",
    "peerjs": "^1.4.7",
    "skeleton-elements": "^4.0.1",
    "spacetime": "^7.2.0",
    "svelte": "^3.53.1",
    "svelte-star-rating": "^2.0.1",
    "swiper": "^8.4.5",
    "url-join": "^5.0.0",
    "xstate": "^4.34.0"
  },
  "devDependencies": {
    "@capacitor/assets": "^2.0.4",
    "@capacitor/cli": "4.6.0",
    "@rollup/plugin-typescript": "8.5.0",
    "@sveltejs/vite-plugin-svelte": "next",
    "@tsconfig/svelte": "^3.0.0",
    "@types/node": "^18.0.3",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "@vitejs/plugin-basic-ssl": "^0.1.2",
    "autoprefixer": "^10.4.13",
    "cordova-res": "^0.15.4",
    "cpy-cli": "^4.2.0",
    "cross-env": "^7.0.3",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "postcss": "^8.4.19",
    "postcss-preset-env": "^7.8.3",
    "svelte-check": "^2.10.2",
    "svelte-preprocess": "^5.0.0",
    "tailwindcss": "^3.2.4",
    "ts-loader": "9.4.2",
    "ts-node": "^10.8.2",
    "tslib": "2.4.1",
    "typescript": "@next",
    "vite": "^3.2.11",
    "workbox-cli": "^6.5.4"
  }
}